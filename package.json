{
  "name": "ts-railway",
  "version": "3.0.5",
  "description": "ROP flavoured Result & AsyncResult types",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "preversion": "npm run lint -- --fix && npm run build && npm t && npm run type-coverage && betterer && typedoc src/index.ts && touch docs/.nojekyll",
    "postversion": "git add . && git commit -am lint || : && git push && git push --tags",
    "build:dts": "tsc -p configs/tsconfig.dts.json",
    "build:esm": "tsc -p configs/tsconfig.esm.json",
    "build:cjs": "tsc -p configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs",
    "watch": "tsc -p . -w",
    "test": "jest",
    "lint": "eslint .",
    "coverage": "codecov",
    "type-coverage": "type-coverage --detail --strict",
    "betterer": "betterer"
  },
  "keywords": [
    "result",
    "railway-oriented-programming",
    "rop"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-railway/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-railway#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-railway.git"
  },
  "devDependencies": {
    "@betterer/cli": "^4.4.1",
    "@betterer/typescript": "^4.4.1",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "pipe-ts": "^0.0.9",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-pipe": "^1.2.3",
    "type-coverage": "^2.18.3",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "blueprint-templates"
    ],
    "testRegex": "\\.spec\\.ts$",
    "transform": {
      "^.+\\.(js|tsx?)$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
