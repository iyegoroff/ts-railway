{
  "name": "ts-railway",
  "version": "6.0.4",
  "description": "ROP flavoured Result & AsyncResult types",
  "sideEffects": false,
  "types": "./dist/dts/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "react-native": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint -- --fix && npm run build && npm t && npm run type-coverage && npm run typedoc",
    "postversion": "git add . && git commit -am \"lint & docs [skip build]\" || : && git push && git push --tags",
    "build:ts": "tsc -p configs/tsconfig.cjs.json && tsc -p configs/tsconfig.mjs.json && tsc -p configs/tsconfig.dts.json",
    "build": "rm -rf dist && npm run build:ts && echo '{ \"type\": \"commonjs\" }' >> dist/cjs/package.json",
    "test": "jest",
    "typedoc": "typedoc src/index.ts && touch docs/.nojekyll",
    "lint": "eslint .",
    "type-coverage": "type-coverage --detail --strict"
  },
  "keywords": [
    "result",
    "railway-oriented-programming",
    "rop"
  ],
  "files": [
    "dist",
    "src",
    "test",
    "mds"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-railway/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-railway#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-railway.git"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^28.1.0",
    "pipe-ts": "^0.0.9",
    "prettier": "^2.6.2",
    "runtypes": "^6.5.1",
    "ts-jest": "^28.0.3",
    "ts-pipe": "^1.2.3",
    "type-coverage": "^2.21.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.(js|tsx?)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "blueprint-templates"
    ],
    "moduleNameMapper": {
      "(.*)\\.js$": "$1"
    },
    "testRegex": "\\.spec\\.ts$",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
